:host {
  display: inline-block;
  font-family: var(--picto-font-family) var(--picto-font-size);
  color: var(--picto-color-base);
  position: relative;
}
:host *,
:host *::before,
:host *::after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-family: inherit;
}
:host h1 {
  font-size: 61px;
  line-height: 75px;
  margin-top: 25px;
  margin-bottom: 50px;
}
:host h2 {
  font-size: 41px;
  line-height: 50px;
  margin-top: 25px;
  margin-bottom: 25px;
}
:host h3 {
  font-size: 27px;
  line-height: 50px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host h4 {
  font-size: 18px;
  line-height: 25px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host h5 {
  font-size: 18px;
  line-height: 25px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host p,
:host ul,
:host ol,
:host pre,
:host table,
:host blockquote {
  margin-top: 0px;
  margin-bottom: 25px;
}
:host ul ul,
:host ol ol,
:host ul ol,
:host ol ul {
  margin-top: 0px;
  margin-bottom: 0px;
}
:host hr {
  border: 1px solid;
  margin: -1px 0;
}
dd {
  white-space: pre;
}
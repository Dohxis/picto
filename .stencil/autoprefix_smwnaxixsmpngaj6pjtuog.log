:host {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  font-family: var(--picto-font-family);
  font-size: calc(var(--picto-font-size) * 1);
  line-height: calc(var(--picto-line-height) * 2);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
:host * {
  margin: 0;
  padding: 0;
  line-height: calc(var(--picto-line-height) * 1);
}
:host *,
:host *::before,
:host *::after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-family: inherit;
}
:host h1 {
  font-size: calc(var(--picto-font-size) * 3.5);
  line-height: calc(var(--picto-line-height) * 4);
  margin-top: 0;
  margin-bottom: calc(var(--picto-line-height) * 2);
}
:host h2 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.6);
  line-height: calc(var(--picto-line-height) * 3);
}
:host h3 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.3);
  line-height: calc(var(--picto-line-height) * 3);
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
:host h4 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 1.7);
  line-height: calc(var(--picto-line-height) * 2);
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
:host p {
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
.pane-left {
  padding: calc(var(--sm-padding-amount) * 3.5);
  width: 300px;
  height: 100vh;
  background: rgba(0,0,0,0.05);
}
.pane-right {
  padding: calc(var(--sm-padding-amount) * 5);
  height: 100vh;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  overflow: scroll;
}
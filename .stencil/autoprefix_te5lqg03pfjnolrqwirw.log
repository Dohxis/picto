@import url("https://fonts.googleapis.com/css?family=Krub:300,500,700");
:host {
  font-family: Krub;
  font-size: 18px;
  line-height: 25px;
}
:host h1 {
  font-size: 61px;
  line-height: 75px;
  margin-top: 25px;
  margin-bottom: 50px;
}
:host h2 {
  font-size: 41px;
  line-height: 50px;
  margin-top: 25px;
  margin-bottom: 25px;
}
:host h3 {
  font-size: 27px;
  line-height: 50px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host h4 {
  font-size: 18px;
  line-height: 25px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host h5 {
  font-size: 18px;
  line-height: 25px;
  margin-top: 25px;
  margin-bottom: 0px;
}
:host p,
:host ul,
:host ol,
:host pre,
:host table,
:host blockquote {
  margin-top: 0px;
  margin-bottom: 25px;
}

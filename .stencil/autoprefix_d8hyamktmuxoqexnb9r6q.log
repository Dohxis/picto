:host {
  display: block;
  position: relative;
  font-family: var(--picto-font-family);
  font-size: calc(var(--picto-font-size) * 1);
  line-height: calc(var(--picto-line-height) * 2);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(90%, transparent), to(rgba(255,0,0,0.4)));
  background: linear-gradient(transparent 90%, rgba(255,0,0,0.4));
  background-size: 100% calc(var(--picto-line-height) * 1);
}
:host * {
  margin: 0;
  padding: 0;
  line-height: calc(var(--picto-line-height) * 1);
  margin-bottom: calc(var(--picto-line-height) * 1);
}
:host *,
:host *::before,
:host *::after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-family: inherit;
}
:host h1 {
  font-size: calc(var(--picto-font-size) * 3.5);
  line-height: calc(var(--picto-line-height) * 4);
  margin-top: 0;
  margin-bottom: calc(var(--picto-line-height) * 2);
}
:host h2 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.6);
  line-height: calc(var(--picto-line-height) * 3);
}
:host h3 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.3);
  line-height: calc(var(--picto-line-height) * 3);
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
:host h4 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 1.7);
  line-height: calc(var(--picto-line-height) * 2);
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
:host p {
  margin-top: calc(var(--picto-line-height) * -0.5);
  margin-bottom: calc(var(--picto-line-height) * 1.5);
}
:host label {
  display: block;
}
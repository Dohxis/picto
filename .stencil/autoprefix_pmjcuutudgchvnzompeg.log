* {
  margin-top: 0;
  margin-bottom: vr(2);
}
*,
*::before,
*::after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-family: inherit;
}
h1 {
  font-size: calc(var(--picto-font-size) * 3.5);
  line-height: vr(4);
  margin-top: 0;
  margin-bottom: vr(2);
}
h2 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.6);
  line-height: vr(3);
}
h3 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 2.3);
  line-height: vr(3);
  margin-top: vr(-0.5);
  margin-bottom: vr(1.5);
}
h4 {
  font-weight: bold;
  font-size: calc(var(--picto-font-size) * 1.7);
  line-height: vr(2);
  margin-top: vr(-0.5);
  margin-bottom: vr(1.5);
}
p {
  margin-top: vr(-0.5);
  margin-bottom: vr(1.5);
}
:host {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  font-family: var(--picto-font-family);
  font-size: calc(var(--picto-font-size) * 1);
  line-height: vr(2);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.pane-left {
  padding: calc(var(--sm-padding-amount) * 3.5);
  width: 300px;
  height: 100vh;
  background: rgba(0,0,0,0.05);
}
.pane-right {
  padding: calc(var(--sm-padding-amount) * 5);
  height: 100vh;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  overflow: scroll;
}
:host {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  font: var(--sm-font-weight-base) var(--sm-font-base);
  color: var(--sm-color-base);
  position: relative;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
:host p,
:host ol,
:host ul,
:host li,
:host dl,
:host dt,
:host dd,
:host blockquote,
:host figure,
:host fieldset,
:host legend,
:host textarea,
:host pre,
:host iframe,
:host hr,
:host h1,
:host h2,
:host h3,
:host h4,
:host h5,
:host h6 {
  margin: 0;
  padding: 0;
}
:host h1,
:host h2,
:host h3,
:host h4,
:host h5,
:host h6 {
  font-size: 100%;
  font-weight: normal;
}
:host ul {
  list-style: none;
}
:host button,
:host input,
:host select,
:host textarea {
  outline: none;
  margin: 0;
}
:host *,
:host *::before,
:host *::after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font: inherit;
}
.pane-left {
  padding: calc(var(--sm-padding-amount) * 3.5);
  width: 300px;
  height: 100vh;
  background: rgba(0,0,0,0.05);
}
.pane-right {
  padding: calc(var(--sm-padding-amount) * 5);
  height: 100vh;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  overflow: scroll;
}
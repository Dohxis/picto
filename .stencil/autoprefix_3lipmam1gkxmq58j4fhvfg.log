@import url("https://fonts.googleapis.com/css?family=Teko:400,600,700");
:root {
  --sm-font-base: 14px/17px Teko, Helvetica, Arial, sans-serif;
  --sm-font-small: 13px/16px Teko, Helvetica, Arial, sans-serif;
  --sm-font-smaller: 12px/14px Teko, Helvetica, Arial, sans-serif;
  --sm-font-smallest: 11px/13px Teko, Helvetica, Arial, sans-serif;
  --sm-font-big: 16px/19px Teko, Helvetica, Arial, sans-serif;
  --sm-font-bigger: 28px/34px Teko, Helvetica, Arial, sans-serif;
  --sm-font-biggest: 36px/43px Teko, Helvetica, Arial, sans-serif;
  --sm-font-weight-base: 400;
  --sm-font-weight-bold: 700;
  --sm-color-base: rgba(0,0,0,0.5);
  --sm-margin-amount: 10px;
  --sm-padding-amount: 10px;
}

:host {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  font: var(--sm-font-base);
}
.pane-left {
  padding: calc(var(--sm-padding-amount) * 3.5);
  width: 300px;
  height: 100vh;
  background: rgba(0,0,0,0.05);
}
.pane-right {
  padding: calc(var(--sm-padding-amount) * 5);
  height: 100vh;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  overflow: scroll;
}